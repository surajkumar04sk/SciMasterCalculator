<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.stechtricker.scimastercalculator">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MultiCalculator">

        <activity android:name="com.stechtricker.scimastercalculator.MainActivity"
            android:exported="true" />
        <activity android:name="com.stechtricker.scimastercalculator.Contact"
            android:exported="false" />
        <activity android:name="com.stechtricker.scimastercalculator.ScientificCalculator"
            android:exported="false" />
        <activity android:name="com.stechtricker.scimastercalculator.EMICalculator"
            android:exported="false" />
        <activity android:name="com.stechtricker.scimastercalculator.AgeCalculator"
            android:exported="false" />
        <activity android:name="com.stechtricker.scimastercalculator.DiscountCalculator"
            android:exported="false" />
        <activity android:name="com.stechtricker.scimastercalculator.BMIResult"
            android:exported="false" />
        <activity android:name="com.stechtricker.scimastercalculator.BMICalculator"
            android:exported="false" />
        <activity android:name="com.stechtricker.scimastercalculator.PercentageCalculator"
            android:exported="false" />
        <activity android:name="com.stechtricker.scimastercalculator.NormalCalculator"
            android:exported="false" />
        <activity android:name="com.stechtricker.scimastercalculator.SplashScreen"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-3940256099942544~3347511713" />
    </application>

</manifest>
